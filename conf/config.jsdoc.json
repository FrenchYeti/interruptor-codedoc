{
    "source": {
        "includePattern": ".+\\.ts(doc|x)?$",
        "excludePattern": ".*(node_modules|webserver).*"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "public/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "docdash": {
        "static": true,
        "sort": true,
        "disqus": "",
	"meta": {
		"title": "Interruptor API documentation",
		"description": "Official documentation for Interruptor hook library",
		"keyword": "dexcalibur, interruptor, frida, reversense, documentation, hook, android"
	},
        "search": true,
        "collapse": true,
        "typedefs": true,
        "removeQuotes": "none",
        "scripts": [],
        "menu":{
            "Github repo":{
                "href":"https://github.com/frenchyeti/interruptor",
                "target":"_blank",
                "class":"menu-item",
                "id":"repository"
            }
        }
    }
}
